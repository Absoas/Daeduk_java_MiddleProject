<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE sqlMap
			PUBLIC "-//ibatis.apache.org//DTD SQL Map 2.0//EN"
			"http://ibatis.apache.org/dtd/sql-map-2.dtd">
<sqlMap namespace="corRegno">
	<select id="selectCorRegno" resultClass="kr.or.ddit.corRegno.CorRegnoVO">
		select * from corperation
	</select>
	<select id="searchCorRegno" resultClass="kr.or.ddit.corRegno.CorRegnoVO" parameterClass="HashMap">
		select * from corperation where cor_no = #cor_no#
	</select>
	<select id="searchCorName" resultClass="kr.or.ddit.corRegno.CorRegnoVO" parameterClass="HashMap">
		select * from corperation where cor_name=#cor_name#
	</select>

</sqlMap>			





